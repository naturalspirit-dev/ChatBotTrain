// Add 3 seconds to [.variable] and assign to [.result]
.variable:date:"2022-01-01T23:59:59"
.result

// Assigning the result of [math.add] to the [.result] node
set-value:x:@.result

   // Adding 3 seconds to our [.variable] node
   math.add
      get-value:x:@.variable
      time
         seconds:3

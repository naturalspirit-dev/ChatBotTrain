// Read CSV data from /customers.csv and insert first_name and last_name into customers table in crm database

// Loading CSV file
io.file.load:/customers.csv

// Converting to lambda object
csv2lambda:x:@io.file.load

// Connecting to database
data.connect:crm

   // Looping through each CSV record
   for-each:x:@csv2lambda/*

      // Inserting into database
      data.create
         table:customers
         values
            first_name:x:@.dp/#/*/name
            last_name:x:@.dp/#/*/name

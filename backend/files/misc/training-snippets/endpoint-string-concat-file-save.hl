// Create an endpoint that concatenates two arguments and saves the concatenated result to '/result.txt'
.arguments
   argument1:string
   argument2:string

// Concatenating our arguments
strings.concat
   get-value:x:@.arguments/*/argument1
   get-value:x:@.arguments/*/argument2

// Saving the concatenated result to /result.txt
io.file.save:/result.txt
   get-value:x:@strings.concat

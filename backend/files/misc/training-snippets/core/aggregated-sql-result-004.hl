// Joins staff with address from movies database and selects first_name as First Name, last_name as Last Name and address as Address
data.connect:movies
   data.select:@"select s.first_name as 'First Name', s.last_name as 'Last Name', a.address as 'Address'
  from staff as s
    join address as a on a.address_id = s.address_id;"

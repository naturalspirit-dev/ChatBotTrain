// Add nodes resulting from two expressions leading to [.src1] and [.src2] children into [.destination] | Add nodes into [.destination] taken from [.src1] and [.src2]
.src1
   foo1:bar1
   foo2:bar2
.src2
   foo3:bar3
   foo4:bar4
.destination

// Adding all children nodes from both [.src1] and [.src2] into [.destination]
add:x:@.destination
   get-nodes:x:@.src1/*
   get-nodes:x:@.src2/*

// Endpoint allowing user to upload file | File upload endpoint | Create endpoint for uploading files
.arguments
   file
      name:string
      stream:*

// Storing file in etc folder
strings.concat
   .:/etc/
   get-value:x:@.arguments/*/name

// Saving stream to full path for file
io.stream.save-file:x:@strings.concat
   get-value:x:@.arguments/*/file/*/stream


// Script counting lines of code in specified folder.
.folder:/modules/system/
unwrap:x:+/*
signal:magic.io.file.load-recursively
   .:x:@.folder
.no:int:0
for-each:x:@signal/*
   strings.split:x:@.dp/#/0
      .:@"
"
   math.increment:x:@.no
      get-count:x:@strings.split/*
remove-nodes:x:@signal/*

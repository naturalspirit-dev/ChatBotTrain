// Iterate through each child node of [.data] and add all nodes found in [.data] as children nodes to each node found in [.data]
.src
   foo1:bar1
   foo2:bar2
.data
   foo1:bar1
   foo2:bar2

// Iterating through each node in [.data]
for-each:x:@.data/*

   // Adding into currently iterated node all nodes found in [.src]
   add:x:@.dp/#
      get-nodes:x:@.src/*

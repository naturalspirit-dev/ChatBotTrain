{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "magic": {
    "caching": {
      "expiration": 5,
      "expiration-type": "sliding"
    },
    "databases": {
      "mysql": {
        "generic": "Server=127.0.0.1;Database={database};Uid=root;Pwd=ThisIsNotAGoodPassword;SslMode=Preferred;Old Guids=true;"
      },
      "mssql": {
        "generic": "Server=localhost\\SQLEXPRESS;Database={database};Trusted_Connection=True;"
      },
      "default": "mysql"
    },
    "auth": {
      "database": "magic",
      "secret": "THIS-IS-NOT-A-GOOD-SECRET-PLEASE-CHANGE-IT",
      "valid-minutes": 120
    },
    "io": {
      "root-folder": "~/files/"
    },
    "endpoint": {
      "root-folder": "~/files/"
    },
    "tasks": {
      "database": "magic",
      "scheduler": {
        "auto-start": false
      }
    },
    "logging": {
      "database": "magic"
    },
    "license": "SUPPLY-YOUR-LICENSE-HERE"
  }
}
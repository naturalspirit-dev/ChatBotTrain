{
  "magic": {
    "frontend": {
      "urls": ""
    },
    "smtp": {
      "host": null,
      "port": 465,
      "secure": true,
      "username": null,
      "password": null,
      "from": {
        "name": null,
        "address": null
      }
    },
    "lambda": {
      "maxWhileIterations": 5000
    },
    "databases": {
      "mysql": {
        "generic": "Server=db;Database={database};Uid=root;Pwd=ThisIsNotAGoodPassword;SslMode=Preferred;Old Guids=true;"
      },
      "pgsql": {
        "generic": "User ID=postgres;Password=ThisIsNotAGoodPassword;Host=localhost;Port=5432;Database={database}"
      },
      "mssql": {
        "generic": "Server=localhost\\SQLEXPRESS;Database={database};Trusted_Connection=True;"
      },
      "default": "mysql"
    },
    "auth": {
      "secret": "THIS-IS-NOT-A-GOOD-SECRET-PLEASE-CHANGE-IT",
      "https-only": false,
      "valid-minutes": 120,
      "authentication": "crypto.password.verify",
      "ldap": "LDAP://foo.acme.somewhere",
      "auto-auth": null,
      "registration": {
        "allow": true,
        "confirm-email": null
      }
    },
    "logging": {
      "level": "debug",
      "service": "magic.lambda.logging.services.Logger"
    },
    "caching": {
      "service": "magic.lambda.caching.services.MagicMemoryCache"
    },
    "io": {
      "root-folder": "~/files/",
      "file-service": "magic.node.services.FileService",
      "folder-service": "magic.node.services.FolderService",
      "stream-service": "magic.node.services.StreamService"
    },
    "tasks": {
      "scheduler": true
    },
    "sockets": {
      "url": "/sockets"
    }
  }
}